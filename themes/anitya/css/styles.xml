<?xml version="1.0"?>
<styles version="1.0">
    <set name="Global Colors">
        <style name="Primary brand color" variable="brand-primary" type="color" />
        <style name="Secondary brand color" variable="brand-secondary" type="color" />
        <style name="Tertiary brand color" variable="brand-tertiary" type="color" />
        <style name="Quaternary brand color" variable="brand-quaternary" type="color" />
        <style name="Brand white" variable="brand-white" type="color" />
        <style name="Brand black" variable="brand-black" type="color" />
        <style name="Brand success" variable="brand-success" type="color" />
        <style name="Brand info" variable="brand-info" type="color" />
        <style name="Brand warning" variable="brand-warning" type="color" />
        <style name="Brand danger" variable="brand-danger" type="color" />
    </set>
    <set name="Global Page">
        <style name="Body Background" variable="body-bg" type="color" />
        <style name="Body Pattern" variable="body-bg-pattern" type="image" />
        <style name="Text" variable="body" type="type" />
        <style name="Link" variable="link" type="color" />
    </set>
    <set name="Header">
        <style name="Navigation top" variable="top-nav-top" type="size" />
        <style name="Navigation height" variable="top-nav-height" type="size" />
        <style name="Navigation height when fixed" variable="top-nav-height-fixed" type="size" />
        <style name="Header Image Height" variable="intro-image-height" type="size" />
        <style name="Header image default" variable="intro-image-default" type="image" />
    </set>
    <set name="Headings">
        <style name="Heading 1" variable="h1" type="type" />
        <style name="Heading 2" variable="h2" type="type" />
        <style name="Heading 3" variable="h3" type="type" />
        <style name="Heading 4" variable="h4" type="type" />
        <style name="Heading 5" variable="h5" type="type" />
        <style name="Heading 6" variable="h6" type="type" />
    </set>
    <set name="Footer">
        <style name="Link Color" variable="footer-link" type="color" />
        <style name="Link Hover Color" variable="footer-link-hover" type="color" />
        <style name="Section Divider Color" variable="footer-section-divider" type="color" />
    </set>
</styles>
